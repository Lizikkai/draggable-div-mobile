    "name": "Lizikkai",
    "email": "1980372249@qq.com"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.28.0",
    "postcss-px-to-viewport": "^1.1.1",
    "sass": "^1.92.1",
    "typescript": "^4.9.0",
    "vite": "^4.0.0",
    "vue": "3.2.47",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^1.0.0"
