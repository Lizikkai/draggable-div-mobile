{
  "name": "draggable-div-mobile",
  "version": "1.0.0",
  "description": "移动端可拖拽的div",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "vite build --mode lib && vue-tsc --project tsconfig.build.json",
    "build:test": "vite build --mode test && vue-tsc --project tsconfig.build.json",
    "prepublishOnly": "npm run build:lib",
    "format": "prettier --write src/",
    "lint": "eslint src --ext .vue,.js,.ts",
    "lint:fix": "eslint src --ext .vue,.js,.ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lizikkai/draggable-div-mobile.git"
  },
